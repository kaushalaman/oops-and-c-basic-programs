#include <cmath>
#include <cstdio>
#include <vector>
#include <cstdlib>
#include <string.h>
#include <iostream>
#include <algorithm>
#define MAX 1000000
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int a[MAX];
    int t,n,m,x,y,k,l,index;
    cin>>t;
    while(t){
        cin>>n>>m;
        k=1;
        l=n;
        x=0;y=0;index=0;
        for(int i=1;i<=n;i++)
        {
            a[i]=0;
        }
        for(int i=1;i<=m;i++)
        {
            if(i%2!=0){
                x=a[k];
                index=k;
                a[k]+=1;
                if(i%n==0){
                    l=n;
                    k=1;
                }
                else{
                    k++;
                }
            }
            else if(i%2==0)
            {
                y=a[l];
                index=l;
                a[l]+=1;
                if(i%n==0)
                {
                    l=n;
                    k=1;
                }
                else{
                    l--;
                }
            }
        }

        if(m%2==0)
        {
            cout<<index<<" "<<y<<endl;
        }
        else if(m%2!=0)
        {
            cout<<index<<" "<<x<<endl;
        }
        t--;
    }
    return 0;
}
